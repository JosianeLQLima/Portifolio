# TABUADA DE 1 A 10

numero1=1
numero2=1
resultado=0
print("   Tabuada ")
while numero1<=10:
    print("\n")
    for resultado in range(10):
          resultado = numero1 * numero2
          print(f" {numero1} * {numero2} = {resultado}")
          numero2 = numero2 + 1

    numero1 = numero1 + 1
    numero2 = 1
    continue
else:
    print("fim")
